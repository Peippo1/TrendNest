DATA_SOURCE=data/sample.csv
EXPORT_PATH=data/cleaned_data.csv
GEMINI_API_KEY=your_gemini_key_here

# Local path to your GCP service account JSON (keep the file outside git)
GOOGLE_APPLICATION_CREDENTIALS=.gcp/your-service-account.json
GCP_PROJECT_ID=your_project_id
BQ_DATASET=your_dataset
BQ_TABLE=your_table

# Logging / observability
LOG_LEVEL=INFO
ENVIRONMENT=dev
# Set OTEL_EXPORTER_OTLP_ENDPOINT to send traces to a collector; otherwise spans log to stdout
# OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4318/v1/traces
# Optional headers for auth (comma-separated k=v)
# OTEL_EXPORTER_OTLP_HEADERS=Authorization=Bearer YOUR_TOKEN
